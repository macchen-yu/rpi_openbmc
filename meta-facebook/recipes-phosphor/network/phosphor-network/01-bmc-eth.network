[Match]
Driver=ftgmac100

[Network]
DHCP=true
LinkLocalAddressing=ipv6
IPv6AcceptRA=1
IPv6LinkLocalAddressGenerationMode=eui64
[DHCP]
ClientIdentifier=mac
UseDNS=true
UseDomains=true
UseNTP=true
UseHostname=true
SendHostname=true
[DHCPv6]
ClientIdentifier=duid
DUIDType=link-layer
IAID=0
[IPv6AcceptRA]
DHCPv6Client=true
